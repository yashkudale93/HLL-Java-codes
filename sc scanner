import java.util.Scanner;

public class util {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String name = sc.next();
        System.out.println(name);
       
    }
}

output
Tony


import java.util.Scanner;

public class util {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String name = sc.nextLine();
        System.out.println(name);
       
    }
}

Output
Tony stark

