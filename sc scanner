import java.util.Scanner;

public class util {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String name = sc.next();
        System.out.println(name);
       
    }
}

output
Tony


import java.util.Scanner;

public class util {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String name = sc.nextLine();
        System.out.println(name);
       
    }
}

Output
Tony stark

import java.util.Scanner;

public class util {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        int b = sc.nextInt();
        int sum = a + b;
        System.out.println(sum);
       
    }
}

Output

5
10 
=15

